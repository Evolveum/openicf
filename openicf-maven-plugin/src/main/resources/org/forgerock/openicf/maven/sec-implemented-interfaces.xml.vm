<section xml:id='sec-implemented-interfaces-${connectorName}-${project.version}'
         xmlns='http://docbook.org/ns/docbook'
         version='5.0' xml:lang='en'
         xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
         xsi:schemaLocation='http://docbook.org/ns/docbook
 http://docbook.org/xml/5.0/xsd/docbook.xsd'
         xmlns:xlink='http://www.w3.org/1999/xlink'>

    <title>OpenICF Interfaces Implemented by the ${connectorDisplayName}</title>
    <para>
        The ${connectorDisplayName} implements the following OpenICF interfaces.
    </para>

    <!--THIS LIST IS BUILT FROM THE IMPLEMENTED INTERFACES, LINKS BUILT FROM THE
    CORRESPONDING IDS IN appendix-interfaces.xml where more info is provided for
    each interface-->

    <variablelist>
        #foreach($action in $connectorInterfaces)
            #if ("AuthenticationApiOp" == $action)
                <varlistentry>
                    <term>
                        <link xlink:show="new"
                              xlink:href="config-reference#interface-attributeNormalizer"
                              xlink:role="http://docbook.org/xlink/role/olink">AttributeNormalizer
                        </link>
                    </term>
                    <listitem>
                        <para>
                            Provides simple authentication with two parameters, presumed to be a user
                            name and password.
                        </para>
                    </listitem>
                </varlistentry>
            #elseif ("CreateApiOp" == $action)
