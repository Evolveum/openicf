<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE Waveset PUBLIC 'waveset.dtd' 'waveset.dtd'>
<Waveset>
	<Configuration name='MySQLUserConnector-PostProcessing'>
		<Extension>
		    <List>
		    
			<Object>
			
			    <!--  Apply only if conector is version 1.0.0.* -->
			    <Attribute name='version' >
			      <List>
			        <Object>
			          <Attribute name='versionSegment' value='major' />
			          <Attribute name='operator' value='eq' />
			          <Attribute name='operand' value='1' />
			        </Object>
			        <Object>
			          <Attribute name='versionSegment' value='minor' />
			          <Attribute name='operator' value='eq' />
			          <Attribute name='operand' value='0' />
			        </Object>
			        <Object>
			          <Attribute name='versionSegment' value='patch' />
			          <Attribute name='operator' value='eq' />
			          <Attribute name='operand' value='0' />
			        </Object>
			      </List>
			    </Attribute>
                
                <Attribute name="replace">
                  <Map>
                    <MapEntry key='typeString' value='MySQL' /> <!-- Replace name for MySQL RA  -->
                  </Map>
                </Attribute>
                			    			  
			</Object>
			
			</List>
			
		</Extension>

   	 </Configuration>

</Waveset>
