<!-- 

    Sample output:
    
    INFORMATION FOR GROUP DFPADMN
        SUPERIOR GROUP=SYSADMN      OWNER=SYSADMN   CREATED=06.123 
        NO INSTALLATION DATA
        NO MODEL DATA SET
        TERMUACC
        SUBGROUP(S)= DFPGRP1, DFPGRP2
        USER(S)=      ACCESS=      ACCESS COUNT=     UNIVERSAL ACCESS=
        IBMUSER         JOIN          000000              ALTER
           CONNECT   ATTRIBUTES=NONE
           REVOKE DATE=NONE                 RESUME DATE=NONE
        DSMITH          JOIN          000002              READ
          CONNECT    ATTRIBUTES=NONE
          REVOKE DATE=NONE                  RESUME DATE=NONE
        HOTROD          CONNECT       000004              READ
          CONNECT    ATTRIBUTES=ADSP SPECIAL OPERATIONS
          REVOKE DATE=NONE                  RESUME DATE=NONE
        ESHAW           USE           000000              READ
          CONNECT    ATTRIBUTES=NONE
          REVOKE DATE=NONE                  RESUME DATE=NONE
        PROJECTB        USE           000000              READ
          CONNECT    ATTRIBUTES=NONE
          REVOKE DATE=NONE                  RESUME DATE=NONE
        ADM1            JOIN          000000              READ
          CONNECT    ATTRIBUTES=OPERATIONS
          REVOKE DATE=NONE                  RESUME DATE=NONE
        AEHALL          USE           000000              READ
          CONNECT    ATTRIBUTES=REVOKED
          REVOKE DATE=NONE                  RESUME DATE=NONE
     DFP INFORMATION
        MGMTCLAS= DFP2MGMT
        STORCLAS= DFP2STOR
        DATACLAS= DFP2DATA
        DATAAPPL= DFP2APPL
 -->

<MapTransform>
  <PatternNode key='RACF.USERID' pattern='USER=(\w{1,8})' optional='false' reset='false'/>
  <PatternNode key='RACF.NAME' pattern='NAME=(.*?)\s+(?=OWNER=)' optional='false' reset='false'/>
  <PatternNode key='RACF.OWNER' pattern='OWNER=(\w{1,8})' optional='false' reset='false'>
    <SubstituteTransform pattern='^$' substitute='UNKNOWN'/>
  </PatternNode>
  <PatternNode key='RACF.DFLTGRP' pattern='DEFAULT-GROUP=(\w{1,8})' optional='false' reset='false'/>
  <PatternNode key='RACF.PASSDATE' pattern='PASSDATE=(\S{0,6})' optional='false' reset='false'/>
  <PatternNode key='RACF.PASSWORD INTERVAL' pattern='PASS-INTERVAL=(\S*)' optional='false' reset='false'/>
  <PatternNode key='RACF.PHRASEDATE' pattern='PHRASEDATE=(.*?)\s+\n' optional='true' reset='false'/>
  <PatternNode key='RACF.ATTRIBUTES' pattern='((ATTRIBUTES=.*\n\s*)+)' optional='true' reset='false'>
    <SubstituteTransform pattern='ATTRIBUTES=(\S+)\s+' substitute='$1 '/>
    <SubstituteTransform pattern='(.*)\s' substitute='$1'/>
    <SubstituteTransform pattern='^$' substitute='NONE'/>
    <SplitTransform splitPattern='\s+'/>
  </PatternNode>
        //"  <PatternNode key='RACF.CLAUTH' pattern='CLASS AUTHORIZATIONS=([^\n]*(\s{23}.+\n)*)' optional='true' reset='false'>
        //"    <SubstituteTransform pattern='(.*)\s' substitute='$1'/>
        //"    <SplitTransform splitPattern='\s+'/>
        //"  </PatternNode>
  <PatternNode key='RACF.DATA' pattern='INSTALLATION-DATA=([^\n]*(\s{20}.+\n)*)' optional='true' reset='false'>
    <SubstituteTransform pattern='^(.{50})[^\n]+' substitute='$1'/>
    <SubstituteTransform pattern='\n\s{20}(.{50})[^\n]+' substitute='$1'/>
    <SubstituteTransform pattern='\n' substitute=''/>
    <SubstituteTransform pattern='^$' substitute='NO-INSTALLATION-DATA'/>
  </PatternNode>
  <PatternNode key='RACF.GROUPS' pattern='((?:\s+GROUP=\w+\s+AUTH=\w*\s+CONNECT-OWNER=(?:[^\n]+\n){4})+)' optional='true' reset='true'>
    <SubstituteTransform pattern='.*?GROUP=(\w+)\s+AUTH=.+?CONNECT-OWNER=\w+([^\n]+\n){4}' substitute='$1 '/>
    <SubstituteTransform pattern='^\s+(.*)' substitute='$1'/>
    <SubstituteTransform pattern='(\s+)$' substitute=''/>
    <SplitTransform splitPattern='\s+'/>
  </PatternNode>
  <PatternNode key='RACF.CONNECT-OWNERS' pattern='((?:\s+GROUP=\w+\s+AUTH=\w*\s+CONNECT-OWNER=(?:[^\n]+\n){4})+)' optional='true' reset='false'>
    <SubstituteTransform pattern='.*?GROUP=\w+\s+AUTH=.+?CONNECT-OWNER=(\w+)([^\n]+\n){4}' substitute='$1 '/>
    <SubstituteTransform pattern='^\s+(.*)' substitute='$1'/>
    <SubstituteTransform pattern='(\s+)$' substitute=''/>
    <SplitTransform splitPattern='\s+'/>
  </PatternNode>
</MapTransform>
